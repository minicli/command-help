{
    "name": "minicli/command-help",
    "type": "library",
    "description": "A help command for Minicli",
    "license": "MIT",
    "homepage": "https://github.com/minicli/command-help",
    "keywords": ["cli","command-line", "minicli"],
    "autoload": {
        "psr-4": {
            "Command\\": "Command/"
        }
    },
    "require": {
        "minicli/minicli": "^4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "pestphp/pest": "^2.6"
    },
    "scripts": {
        "lint": ["pint"],
        "test": ["pest"]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
